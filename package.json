{
  "name": "react-webpack-starter-kit",
  "version": "1.0.0",
  "private": true,
  "repository": "git@github.com:kreshjaque/react-webpack-starter-kit.git",
  "author": "Krishna Kumar <kreshjaque@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@testing-library/react": "^11.1.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.5.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.7",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "html-loader": "^1.3.2",
    "husky": "^4.2.3",
    "jest": "^26.5.3",
    "lint-staged": "^10.0.7",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "react-test-renderer": "^16.12.0",
    "stylelint": "^13.2.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "webpack": "^5.1.1",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^5.2.0"
  },
  "scripts": {
    "start": "webpack-dashboard -- webpack-dev-server --open --config config/webpack.dev.js",
    "build": "webpack  --config /config/webpack.prod.js",
    "lint:js": "eslint src/**/*.js",
    "prettier:fix": "prettier --write src/",
    "test": "jest",
    "lint:css": "stylelint src/**/*.styles.js"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^17.0.0-rc.2",
    "clean-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^4.5.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.19",
    "styled-components": "^5.0.1",
    "webpack-dashboard": "^3.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint",
      "pretty-quick — staged"
    ],
    "config/*.js": [
      "eslint",
      "pretty-quick — staged"
    ]
  }
}
